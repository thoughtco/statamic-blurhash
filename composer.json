{
    "name": "thoughtco/statamic-blurhash",
    "description": "A Statamic add-on to generate and decode blurhash images",
    "autoload": {
        "psr-4": {
            "Thoughtco\\BlurHash\\": "src"
        }
    },
    "authors": [
        {
            "name": "Ryan Mitchell"
        }
    ],
    "support": {
        "email": "help@thoughtcollective.com"
    },
    "extra": {
        "statamic": {
            "name": "BlurHash",
            "description": "Generate and decode blurhash images from Statamic assets"
        },
        "laravel": {
            "providers": [
                "Thoughtco\\BlurHash\\ServiceProvider"
            ]
        }
    },
    "require": {
        "bepsvpt/blurhash": "^3.0.0",
        "laravel/pint": "^1.16"
    }
}
